{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}